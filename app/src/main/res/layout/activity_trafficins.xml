<?xml version="1.0" encoding="utf-8"?>
<com.example.administrator.droideye.TrafficMonitor.TrafficInsView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/showtrafficview"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- This picture is for joking.-->
    <ImageButton
        android:id="@+id/rudder"
        android:background="#00000000"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layoutAnimation="@anim/anim_listview"
        android:layout_marginLeft="8dip"
        android:layout_marginTop="6dip"
        android:layout_marginBottom="6dip"
        android:src="@drawable/btn_rudder"
        />


    <ListView
        android:id="@+id/trafficList"
        android:layout_below="@id/rudder"
        android:layout_marginLeft="8dip"
        android:layout_marginRight="8dip"
        android:divider="@null"
        android:scrollbars="none"
        android:layoutAnimation="@anim/anim_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</com.example.administrator.droideye.TrafficMonitor.TrafficInsView>
